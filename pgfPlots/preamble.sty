\ProvidesPackage{preamble}
%--- Used to precompile the tikz/pgfplots figures ---
\usepackage{tikz}
\usetikzlibrary{external}
\tikzexternalize[prefix=tikzfigs/] % activate! 
% "tikzfigs/" folder needs to created before hand. It is were the compiled
% images are stored in pdf format.
%----------------------------------------------------
\tikzset{font=\scriptsize}
\usepackage{pgfplots}
\usetikzlibrary{plotmarks}
\pgfplotsset{compat=newest}
\usepgfplotslibrary{groupplots}
\pgfplotsset{every axis/.append style={thick}}
%Smaller Legends
\pgfplotsset{
/pgfplots/legend image code/.code={
\draw[mark repeat=2,mark phase=2]
plot coordinates {
(0pt,0pt)
(5pt,0pt)        %% default is (0.3cm,0cm)
(10pt,0pt)         %% default is (0.6cm,0cm)
};%
}
}
